module.exports = (client) => {
    const channelId = ''
  
    const updateMembers = (guild) => {
      const channel = guild.channels.cache.get(channelId)
      channel.setName(`Bots: ${guild.members.cache.filter(m => m.user.bot).size}`)
    }
  
    client.on('guildMemberAdd', (member) => updateMembers(bot.guild))
    client.on('guildMemberRemove', (member) => updateMembers(bot.guild))
  
    const guild = client.guilds.cache.get('')
    updateMembers(guild)
  }